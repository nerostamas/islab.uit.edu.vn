{{#if @blog.navigation}}


<nav class="nav" role="nav">
    <ul class="list-inline">
      {{#foreach navigation}}
            <li class="nav-{{slug}}{{#if current}} nav-current{{/if}}" role="presentation">
            	<a href="{{url}}">{{label}}</a>
           	</li>
      {{/foreach}}
      
    </ul>

</nav>

{{/if}}
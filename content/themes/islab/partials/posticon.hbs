{{#has tag="video"}}
    <i class="fa fa-film" rel="tooltip" title="Video"></i>
{{/has}}

{{#has tag="photo"}}
    <i class="fa fa-photo" rel="tooltip" title="photo"></i>
{{/has}}

{{#has tag="music"}}
    <i class="fa fa-music" rel="tooltip" title="music"></i>
{{/has}}

{{#has tag="link"}}
    <i class="fa fa-share" rel="tooltip" title="link"></i>
{{/has}}

{{#has tag="copy"}}
    <i class="fa fa-copy" rel="tooltip" title="copy"></i>
{{/has}}

{{^has tag="photo,video,music,link,copy"}}
    <i class="fa fa-align-left"></i>
{{/has}}